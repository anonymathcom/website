{
    "ci": {
        "collect": {
            "url": [
                "http://localhost:3000"
            ],
            "startServerCommand": "http-server ./build/client -p 3000 -g",
            "startServerReadyPattern": "Available on",
            "numberOfRuns": 1
        },
        "upload": {
            "target": "temporary-public-storage"
        },
        "assert": {
            "preset": "lighthouse:recommended"
        }
    }
}